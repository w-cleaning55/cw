# Environment variables for Next.js
# Copy this file to .env and add your actual values

# Builder.io API key (public)
NEXT_PUBLIC_BUILDER_KEY=your_builder_key_here

# API configuration
PING_MESSAGE="ping pong"

# JWT Secret (server-side only) - Generate a secure random string
JWT_SECRET=your_jwt_secret_here

# Database configuration (optional - add as needed)
# DATABASE_URL=your_database_url_here
# SUPABASE_URL=your_supabase_url_here
# SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Vercel deployment will automatically set NODE_ENV=production
