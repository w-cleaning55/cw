{
  "name": "cleaning-world-pro",
  "version": "1.0.0",
  "description": "Professional cleaning services management system with AI chatbot, multi-database support, and Google Business Profile integration",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "next dev",
    "dev:fast": "node scripts/dev-optimized.js",
    "build": "next build",
    "build:fast": "cross-env NEXT_TELEMETRY_DISABLED=1 next build",
    "build:analyze": "cross-env ANALYZE=true npm run build",
    "start": "next start",
    "lint": "next lint --fix",
    "lint:check": "next lint",
    "typecheck": "tsc --noEmit --incremental",
    "typecheck:fast": "tsc --noEmit --incremental --skipLibCheck",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rimraf .next out dist .tsbuildinfo",
    "clean:all": "rimraf .next out dist .tsbuildinfo node_modules package-lock.json",
    "db:seed": "node scripts/seed-database.js",
    "prebuild": "npm run clean",
    "postbuild": "next-sitemap --config next-sitemap.config.cjs"
  },
  "dependencies": {
    "@next/bundle-analyzer": "^15.0.3",
    "@radix-ui/react-accordion": "^1.2.0",
    "@radix-ui/react-alert-dialog": "^1.1.1",
    "@radix-ui/react-aspect-ratio": "^1.1.0",
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-checkbox": "^1.1.1",
    "@radix-ui/react-collapsible": "^1.1.0",
    "@radix-ui/react-context-menu": "^2.2.1",
    "@radix-ui/react-dialog": "^1.1.2",
    "@radix-ui/react-dropdown-menu": "^2.1.1",
    "@radix-ui/react-hover-card": "^1.1.1",
    "@radix-ui/react-label": "^2.1.0",
    "@radix-ui/react-menubar": "^1.1.1",
    "@radix-ui/react-navigation-menu": "^1.2.0",
    "@radix-ui/react-popover": "^1.1.1",
    "@radix-ui/react-progress": "^1.1.0",
    "@radix-ui/react-radio-group": "^1.2.0",
    "@radix-ui/react-scroll-area": "^1.1.0",
    "@radix-ui/react-select": "^2.1.1",
    "@radix-ui/react-separator": "^1.1.0",
    "@radix-ui/react-slider": "^1.2.0",
    "@radix-ui/react-slot": "^1.1.0",
    "@radix-ui/react-switch": "^1.1.0",
    "@radix-ui/react-tabs": "^1.1.0",
    "@radix-ui/react-toast": "^1.2.1",
    "@radix-ui/react-toggle": "^1.1.0",
    "@radix-ui/react-toggle-group": "^1.1.0",
    "@vercel/analytics": "^1.5.0",
    "@vercel/speed-insights": "^1.2.0",
    "bcryptjs": "^3.0.2",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.0.0",
    "input-otp": "^1.2.4",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.462.0",
    "next": "^15.0.3",
    "next-themes": "^0.3.0",
    "react": "^18.3.1",
    "react-day-picker": "^8.10.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.53.0",
    "react-resizable-panels": "^2.1.3",
    "sonner": "^1.5.0",
    "tailwind-merge": "^2.5.2",
    "tailwindcss-animate": "^1.0.7",
    "uuid": "^11.1.0",
    "vaul": "^0.9.3"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.5.15",
    "@types/bcryptjs": "^2.4.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^22.5.5",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.8.0",
    "@typescript-eslint/parser": "^8.8.0",
    "autoprefixer": "^10.4.21",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-next": "^15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react": "^7.35.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "next-sitemap": "^4.2.3",
    "postcss": "^8.5.6",
    "prettier": "^3.5.3",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.11",
    "typescript": "^5.5.3",
    "vitest": "^3.1.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md,mdx,css,html,yml,yaml}": [
      "prettier --write"
    ]
  },
  "browserslist": {
    "production": [
      ">0.3%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "engines": {
    "node": ">=18.17.0",
    "npm": ">=9.0.0"
  },
  "keywords": [
    "cleaning-services",
    "saudi-arabia",
    "next.js",
    "typescript",
    "react",
    "tailwindcss",
    "ai-chatbot",
    "multi-database",
    "google-business"
  ],
  "author": {
    "name": "Cleaning World",
    "email": "info@m-clean.net",
    "url": "https://m-clean.net"
  },
  "license": "MIT",
  "homepage": "https://m-clean.net",
  "repository": {
    "type": "git",
    "url": "https://github.com/cleaning-world/cleaning-world-pro"
  }
}
